<!DOCTYPE html>
<html lang="en">
<head>
  <meta charset="UTF-8">
  <title>Checkout - Pet Karkos</title>
  <link rel="stylesheet" href="style.css">
</head>
<body>
  <header>
    <h1>Checkout - Pet Karkos</h1>
  </header>

  <section>
    <h2>Your Cart</h2>
    <ul id="cart-list"></ul>
    <p><strong>Total:</strong> $<span id="total">0.00</span></p>
  </section>

  <section>
    <h2>Customer Details</h2>
    <form onsubmit="submitOrder(event)">
      <input type="text" placeholder="Full Name" required><br><br>
      <input type="email" placeholder="Email" required><br><br>
      <input type="text" placeholder="Address" required><br><br>
      <button type="submit">Place Order</button>
    </form>
  </section>

  <script>
    const cart = JSON.parse(localStorage.getItem('cart') || '[]');
    const list = document.getElementById('cart-list');
    let total = 0;

    cart.forEach(item => {
      const li = document.createElement('li');
      li.textContent = `${item.product} - $${item.price.toFixed(2)}`;
      list.appendChild(li);
      total += item.price;
    });

    document.getElementById('total').textContent = total.toFixed(2);

    function submitOrder(e) {
      e.preventDefault();
      alert('Order submitted! Thank you for shopping with Pet Karkos.');
      localStorage.removeItem('cart');
      window.location.href = 'index.html';
    }
  </script>
</body>
</html>
